{
  "alias": [
    "jameswlane.com",
    "www.jameswlane.com"
  ],
  "build": {
    "env": {
      "GITHUB_TOKEN": "@github-api-token",
      "NODE_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    },
    {
      "src": "/lambdas/contact",
      "use": "@now/node"
    }
  ],
  "env": {
    "SENDGRID_API_KEY": "@sendgrid_api_key"
  },
  "name": "jameswlane.com",
  "routes": [
    {
      "src": "/api/contact",
      "dest": "/lambdas/contact"
    }
  ],
  "version": 2
}
